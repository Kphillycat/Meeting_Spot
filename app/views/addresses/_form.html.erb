<%= form_for @address do |f| %>
	<% if @address.errors.any? %>
		<div id="error_explanation"> 
			<h2> <%= pluralize(@address.errors.count, "error") %> </h2> 
			<ul>
				<% @address.errors.full_messages.each do |message| %>
				<li> <%= message %> </li>
				<% end %>
			</ul>
		</div>
	<% end %>

<% end %>