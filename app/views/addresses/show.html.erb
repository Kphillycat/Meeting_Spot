<h1><%= @address.name %>
<script src="http://maps.googleapis.com/maps/api/js?key=<%= ENV["KEEFS_API_KEY"] %>&sensor=false">
</script>

<script>

function initialize(){
	var mapProp = {
	  center:new google.maps.LatLng(<%= @address.latitude %>, <%= @address.longitude %>),
	  zoom:12,
	  mapTypeId:google.maps.MapTypeId.ROADMAP
	};
	var map = new google.maps.Map(document.getElementById("googleMap"),mapProp);
	var marker = new google.maps.Marker({
  	position:mapProp.center,
 	});

	marker.setMap(map);
}

// var myCenter=new google.maps.LatLng(<%= @address.latitude %>, <%= @address.longitude %>);

google.maps.event.addDomListener(window, 'load', initialize);
</script>

<div id="googleMap" style="width:500px;height:380px;"></div>
